          <xs:attribute name="OutputFile" type="msb:non_empty_string" use="required">
            <xs:annotation>
              <xs:documentation>Gets or sets the target assembly.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="PublicKeyTokens" type="msb:non_empty_string">
