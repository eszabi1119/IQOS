          <xs:attribute name="UseCommandProcessor" />
          <xs:attribute name="Verbosity" type="xs:string">
            <xs:annotation>
              <xs:documentation>[Optional] Display this amount of details in the output: normal, quiet, detailed.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="Version" type="xs:string">
            <xs:annotation>
              <xs:documentation>[Optional] The version of the package to update.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="WorkingDirectory" type="xs:string">
            <xs:annotation>
              <xs:documentation>[Optional] Gets or sets the working directory.</xs:documentation>
            </xs:annotation>
